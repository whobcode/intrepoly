# Cloudflare TURN Server Credentials
# Copy this file to .env and fill in your values
# Get these from Cloudflare Dashboard > Realtime > TURN

TURN_TOKEN_ID=your_turn_token_id_here
TURN_API_TOKEN=your_turn_api_token_here

# TURN API endpoint (do not change)
TURN_API_URL=https://rtc.live.cloudflare.com/v1/turn/keys

# Cloudflare Realtime SFU Credentials
# Get these from Cloudflare Dashboard > Realtime > SFU
SFU_APP_ID=your_sfu_app_id_here
SFU_API_TOKEN=your_sfu_api_token_here

# SFU API endpoint (do not change)
SFU_API_URL=https://rtc.live.cloudflare.com/v1/apps
